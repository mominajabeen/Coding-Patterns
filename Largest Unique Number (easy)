#include <iostream>
#include <unordered_map>
#include <vector>
using namespace std;

class Solution {
public:
    int largestUniqueNumber(vector<int>& A) {
        int maxUnique = -1;
        unordered_map<int, int> freqMap;
        for(auto const& n : A){
            freqMap[n]++;
        }
        for(auto pair : freqMap){
            if(pair.second == 1)
                maxUnique = max(maxUnique, pair.first);
        }
        return maxUnique;
    }
};
