using namespace std;

#include <iostream>
#include <vector>

class Solution {
private:
  static void swap(vector<int>& arr, int i, int j){
    int temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
  }
public:
  int findNumber(vector<int> &nums) {
    int i = 0;
    int missingNumber = nums.size() + 1;
    while(i < nums.size()){
      int j = nums[i] - 1;

      if(nums[i] > 0 && nums[i] != nums[j] && nums[i] <= nums.size()){
        swap(nums, i, j);
      }
      else i++;
    }
    for(int i = 0; i < nums.size(); i++){
      if(nums[i] != i + 1)
        missingNumber = min(missingNumber, i + 1);
    }
    return missingNumber;
  }
};
