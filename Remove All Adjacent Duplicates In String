#include<bits/stdc++.h>
using namespace std;

class Solution {
public:
    string removeDuplicates(string s) {
        stack<char> stack;
        for(int i = 0; i < s.length(); i++){
            if(!stack.empty() && stack.top() == s[i]){
                stack.pop();
            }
            else{
                stack.push(s[i]);
            }
        }
        s = "";
        while(!stack.empty()){
            s += stack.top();
            stack.pop();
        }
        reverse(s.begin(), s.end());
        return s;
    }
};
